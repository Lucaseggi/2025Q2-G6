# LocalStack Configuration
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_DEFAULT_REGION=us-east-1
SQS_ENDPOINT=http://localstack:4566

# Queue URLs (LocalStack format)
PROCESSING_QUEUE_URL=http://localstack:4566/000000000000/processing-queue
EMBEDDING_QUEUE_URL=http://localstack:4566/000000000000/embedding-queue
INSERTING_QUEUE_URL=http://localstack:4566/000000000000/inserting-queue

# OpenSearch Configuration
OPENSEARCH_ENDPOINT=http://opensearch:9200
OPENSEARCH_INDEX=documents

# For AWS deployment, replace with:
# AWS_ACCESS_KEY_ID=your_actual_key
# AWS_SECRET_ACCESS_KEY=your_actual_secret
# AWS_DEFAULT_REGION=us-east-1
# SQS_ENDPOINT=
# PROCESSING_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account/processing-queue
# EMBEDDING_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account/embedding-queue
# INSERTING_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account/inserting-queue
# OPENSEARCH_ENDPOINT=https://your-opensearch-domain.region.es.amazonaws.com