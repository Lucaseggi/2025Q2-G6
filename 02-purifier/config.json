{
  "service": {
    "name": "purifier-ms",
    "version": "1.0.0",
    "debug": false
  },
  "sqs": {
    "queues": {
      "input": "purifying",
      "output": "processing"
    }
  },
  "gemini": {
    "models": [
      "gemini-2.0-flash-lite",
      "gemini-1.5-flash"
    ],
    "rate_limit": 60,
    "max_input_tokens": 1048576,
    "max_output_tokens": 8192,
    "max_retries": 5,
    "retry_delay": 1
  },
  "processing": {
    "batch_size": 1,
    "timeout_seconds": 120,
    "stats_interval_minutes": 5
  }
}
